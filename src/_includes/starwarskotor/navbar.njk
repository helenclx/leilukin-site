{% extends "global/navbar.njk" %}

{% block navbarLinks %}
    {% set navPages = collections.all | eleventyNavigation("KotOR Shrine") %}
    {%- for entry in navPages %}
        <li>
            <a {% if entry.url == page.url %}aria-current="page"{% endif %} href="{{ entry.url }}">{{ entry.title }}</a>
        </li>
    {%- endfor %}
{% endblock %}